    <script src="http://maps.google.com/maps/api/js?key=AIzaSyBdmKUGYS9XHMyjAx7A8Tfa75Er-vFnN0s&sensor=true"></script>
    <script>
      function initialize() {
        if (document.getElementById("map_canvas")) {
          var point = new google.maps.LatLng(39.975196, 116.348597),
          //Settings for the map
          myOptions = {
              zoom: 15,
              center: point,
              mapTypeId: google.maps.MapTypeId.ROADMAP
          },

          //Finding the div we want the map to be in
          mapCanvas = document.getElementById("map_canvas"),
          //Pass in the div and map settings to the map
          map = new google.maps.Map(mapCanvas, myOptions),

          //
          marker = new google.maps.Marker({
              position: point,
              map: map,
              title: "海达网畅在这里"
          });
        }
      }
    </script>

    <title>海达网畅</title>
  </head>

